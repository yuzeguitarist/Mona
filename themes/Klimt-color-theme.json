{
  "$schema": "vscode://schemas/color-theme",
  "name": "Klimt",
  "type": "dark",
  "colors": {
    "editor.background": "#1B0F0C",
    "editor.foreground": "#F3E2C7",
    "editorCursor.foreground": "#D9B44C",
    "editor.selectionBackground": "#3C261D",
    "editor.inactiveSelectionBackground": "#2F1F18",
    "editor.lineHighlightBackground": "#2A1913",
    "editorLineNumber.foreground": "#A78974",
    "editorLineNumber.activeForeground": "#D9B44C",
    "activityBar.background": "#21130F",
    "activityBar.foreground": "#F3E2C7",
    "sideBar.background": "#1E120E",
    "sideBar.foreground": "#F3E2C7",
    "statusBar.background": "#8B5E34",
    "statusBar.foreground": "#120A06",
    "titleBar.activeBackground": "#241510",
    "titleBar.activeForeground": "#F3E2C7",
    "tab.activeBackground": "#1B0F0C",
    "tab.activeForeground": "#F3E2C7",
    "tab.inactiveBackground": "#26150F",
    "tab.inactiveForeground": "#C9AC8C",
    "panel.background": "#1F120E",
    "panel.border": "#3C231B",
    "button.background": "#C5923A",
    "button.foreground": "#120A06",
    "badge.background": "#A66A2B",
    "badge.foreground": "#120A06",
    "input.background": "#24140F",
    "input.border": "#3C231B",
    "focusBorder": "#D9B44C",
    "selection.background": "#3C261D",
    "list.activeSelectionBackground": "#3C261D",
    "list.activeSelectionForeground": "#F3E2C7",
    "list.inactiveSelectionBackground": "#2F1F18",
    "list.hoverBackground": "#2C1A14",
    "list.focusBackground": "#3C261D",
    "list.dropBackground": "#D9B44C33",
    "list.highlightForeground": "#D9B44C",
    "editor.findMatchBackground": "#D9B44C40",
    "editor.findMatchBorder": "#D9B44C",
    "editor.findMatchHighlightBackground": "#D9B44C25",
    "editor.findMatchHighlightBorder": "#D9B44C66",
    "editor.findRangeHighlightBackground": "#D9B44C15",
    "editor.findRangeHighlightBorder": "#D9B44C40",
    "diffEditor.insertedTextBackground": "#88C5A633",
    "diffEditor.removedTextBackground": "#C8A27F33",
    "diffEditor.insertedLineBackground": "#88C5A626",
    "diffEditor.removedLineBackground": "#C8A27F26",
    "diffEditorGutter.insertedLineBackground": "#88C5A6",
    "diffEditorGutter.removedLineBackground": "#C8A27F",
    "diffEditorGutter.modifiedLineBackground": "#E5A85C",
    "diffEditor.diagonalFill": "#A07D6B40",
    "terminal.background": "#1F120E",
    "terminal.foreground": "#F3E2C7",
    "terminalCursor.foreground": "#D9B44C",
    "terminal.ansiBlack": "#51443B",
    "terminal.ansiRed": "#C8A27F",
    "terminal.ansiGreen": "#88C5A6",
    "terminal.ansiYellow": "#E5A85C",
    "terminal.ansiBlue": "#D9B44C",
    "terminal.ansiMagenta": "#C2D196",
    "terminal.ansiCyan": "#88C5A6",
    "terminal.ansiWhite": "#F3E2C7",
    "terminal.ansiBrightBlack": "#8E8580",
    "terminal.ansiBrightRed": "#D6B99F",
    "terminal.ansiBrightGreen": "#A6D4BC",
    "terminal.ansiBrightYellow": "#EAB97D",
    "terminal.ansiBrightBlue": "#E1C370",
    "terminal.ansiBrightMagenta": "#D1DCB0",
    "terminal.ansiBrightCyan": "#A6D4BC",
    "terminal.ansiBrightWhite": "#F5E6CF",
    "gitDecoration.addedResourceForeground": "#88C5A6",
    "gitDecoration.modifiedResourceForeground": "#E5A85C",
    "gitDecoration.deletedResourceForeground": "#C8A27F",
    "gitDecoration.untrackedResourceForeground": "#88C5A6",
    "gitDecoration.ignoredResourceForeground": "#A07D6B",
    "gitDecoration.conflictingResourceForeground": "#C8A27F",
    "gitDecoration.submoduleResourceForeground": "#D9B44C"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A07D6B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#D9B44C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#C2D196"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#F3E2C7"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#E5A85C"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#88C5A6"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#E8C377"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#C8A27F"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#B33A3A"
      }
    }
  ]
}