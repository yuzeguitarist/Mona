{
  "$schema": "vscode://schemas/color-theme",
  "name": "Chopin",
  "type": "light",
  "colors": {
    "editor.background": "#F6F1EA",
    "editor.foreground": "#2E2A36",
    "editorCursor.foreground": "#7A5671",
    "editor.selectionBackground": "#E7D8CC",
    "editor.inactiveSelectionBackground": "#EEE3D8",
    "editor.lineHighlightBackground": "#EFE6DC",
    "editorLineNumber.foreground": "#9B92A0",
    "editorLineNumber.activeForeground": "#7A5671",
    "activityBar.background": "#EDE2D6",
    "activityBar.foreground": "#2E2A36",
    "sideBar.background": "#F1E8DD",
    "sideBar.foreground": "#2E2A36",
    "statusBar.background": "#5C6FA3",
    "statusBar.foreground": "#0F1324",
    "titleBar.activeBackground": "#E2D6C8",
    "titleBar.activeForeground": "#2E2A36",
    "tab.activeBackground": "#F6F1EA",
    "tab.activeForeground": "#2E2A36",
    "tab.inactiveBackground": "#E7DBCE",
    "tab.inactiveForeground": "#746979",
    "panel.background": "#F1E8DF",
    "panel.border": "#D6C8B8",
    "button.background": "#7A5671",
    "button.foreground": "#F6F1EA",
    "badge.background": "#5C6FA3",
    "badge.foreground": "#0F1324",
    "input.background": "#FCF8F2",
    "input.border": "#DCCFC0",
    "focusBorder": "#5C6FA3",
    "selection.background": "#E7D8CC",
    "list.activeSelectionBackground": "#E7D8CC",
    "list.activeSelectionForeground": "#2E2A36",
    "list.inactiveSelectionBackground": "#EEE3D8",
    "list.hoverBackground": "#EEE4DB",
    "list.focusBackground": "#E7D8CC",
    "list.dropBackground": "#5C6FA333",
    "list.highlightForeground": "#5C6FA3",
    "editor.findMatchBackground": "#5C6FA340",
    "editor.findMatchBorder": "#5C6FA3",
    "editor.findMatchHighlightBackground": "#5C6FA325",
    "editor.findMatchHighlightBorder": "#5C6FA366",
    "editor.findRangeHighlightBackground": "#5C6FA315",
    "editor.findRangeHighlightBorder": "#5C6FA340",
    "diffEditor.insertedTextBackground": "#7A9C7133",
    "diffEditor.removedTextBackground": "#AE6F6433",
    "diffEditor.insertedLineBackground": "#7A9C7126",
    "diffEditor.removedLineBackground": "#AE6F6426",
    "diffEditorGutter.insertedLineBackground": "#7A9C71",
    "diffEditorGutter.removedLineBackground": "#AE6F64",
    "diffEditorGutter.modifiedLineBackground": "#BD7E5C",
    "diffEditor.diagonalFill": "#7D6A7A40",
    "terminal.background": "#F1E8DF",
    "terminal.foreground": "#2E2A36",
    "terminalCursor.foreground": "#5C6FA3",
    "terminal.ansiBlack": "#C4BFBD",
    "terminal.ansiRed": "#AE6F64",
    "terminal.ansiGreen": "#7A9C71",
    "terminal.ansiYellow": "#BD7E5C",
    "terminal.ansiBlue": "#5C6FA3",
    "terminal.ansiMagenta": "#7A5671",
    "terminal.ansiCyan": "#7A9C71",
    "terminal.ansiWhite": "#2E2A36",
    "terminal.ansiBrightBlack": "#D9D5D4",
    "terminal.ansiBrightRed": "#C2938B",
    "terminal.ansiBrightGreen": "#9BB594",
    "terminal.ansiBrightYellow": "#CA987D",
    "terminal.ansiBrightBlue": "#7D8CB5",
    "terminal.ansiBrightMagenta": "#9B8094",
    "terminal.ansiBrightCyan": "#9BB594",
    "terminal.ansiBrightWhite": "#4D4A54",
    "gitDecoration.addedResourceForeground": "#7A9C71",
    "gitDecoration.modifiedResourceForeground": "#BD7E5C",
    "gitDecoration.deletedResourceForeground": "#AE6F64",
    "gitDecoration.untrackedResourceForeground": "#7A9C71",
    "gitDecoration.ignoredResourceForeground": "#7D6A7A",
    "gitDecoration.conflictingResourceForeground": "#AE6F64",
    "gitDecoration.submoduleResourceForeground": "#5C6FA3"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9E8E96",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#7A5671",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#7A9C71"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#2E2A36"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#5C6FA3"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#BD7E5C"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#AE6F64"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#7D6A7A"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C43D4D"
      }
    }
  ]
}