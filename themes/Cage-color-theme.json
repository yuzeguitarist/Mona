{
  "$schema": "vscode://schemas/color-theme",
  "name": "Cage",
  "type": "dark",
  "colors": {
    "editor.background": "#0F1317",
    "editor.foreground": "#D9DEE2",
    "editorCursor.foreground": "#8BD0B5",
    "editor.selectionBackground": "#22333C",
    "editor.inactiveSelectionBackground": "#1C2A32",
    "editor.lineHighlightBackground": "#161C22",
    "editorLineNumber.foreground": "#6A7581",
    "editorLineNumber.activeForeground": "#8BD0B5",
    "activityBar.background": "#111820",
    "activityBar.foreground": "#D9DEE2",
    "sideBar.background": "#121920",
    "sideBar.foreground": "#D9DEE2",
    "statusBar.background": "#1A242D",
    "statusBar.foreground": "#D9DEE2",
    "titleBar.activeBackground": "#141C24",
    "titleBar.activeForeground": "#D9DEE2",
    "tab.activeBackground": "#0F1317",
    "tab.activeForeground": "#D9DEE2",
    "tab.inactiveBackground": "#161D24",
    "tab.inactiveForeground": "#7C8793",
    "panel.background": "#10171D",
    "panel.border": "#1F2A33",
    "button.background": "#8BD0B5",
    "button.foreground": "#0C1115",
    "badge.background": "#6EA6C5",
    "badge.foreground": "#0C1115",
    "input.background": "#0F151B",
    "input.border": "#24313B",
    "focusBorder": "#8BD0B5",
    "selection.background": "#22333C",
    "list.activeSelectionBackground": "#22333C",
    "list.activeSelectionForeground": "#D9DEE2",
    "list.inactiveSelectionBackground": "#1C2A32",
    "list.hoverBackground": "#18232A",
    "list.focusBackground": "#22333C",
    "list.dropBackground": "#8BD0B533",
    "list.highlightForeground": "#8BD0B5",
    "editor.findMatchBackground": "#8BD0B540",
    "editor.findMatchBorder": "#8BD0B5",
    "editor.findMatchHighlightBackground": "#8BD0B525",
    "editor.findMatchHighlightBorder": "#8BD0B566",
    "editor.findRangeHighlightBackground": "#8BD0B515",
    "editor.findRangeHighlightBorder": "#8BD0B540",
    "diffEditor.insertedTextBackground": "#8BD0B533",
    "diffEditor.removedTextBackground": "#8DB0D833",
    "diffEditor.insertedLineBackground": "#8BD0B526",
    "diffEditor.removedLineBackground": "#8DB0D826",
    "diffEditorGutter.insertedLineBackground": "#8BD0B5",
    "diffEditorGutter.removedLineBackground": "#8DB0D8",
    "diffEditorGutter.modifiedLineBackground": "#E6C79C",
    "diffEditor.diagonalFill": "#6C768040",
    "terminal.background": "#10171D",
    "terminal.foreground": "#D9DEE2",
    "terminalCursor.foreground": "#8BD0B5",
    "terminal.ansiBlack": "#42464A",
    "terminal.ansiRed": "#8DB0D8",
    "terminal.ansiGreen": "#8BD0B5",
    "terminal.ansiYellow": "#E6C79C",
    "terminal.ansiBlue": "#8BD0B5",
    "terminal.ansiMagenta": "#9BB8FF",
    "terminal.ansiCyan": "#9BB8FF",
    "terminal.ansiWhite": "#D9DEE2",
    "terminal.ansiBrightBlack": "#848789",
    "terminal.ansiBrightRed": "#AAC4E2",
    "terminal.ansiBrightGreen": "#A8DCC8",
    "terminal.ansiBrightYellow": "#EBD2B0",
    "terminal.ansiBrightBlue": "#A2D9C4",
    "terminal.ansiBrightMagenta": "#B4CAFF",
    "terminal.ansiBrightCyan": "#B4CAFF",
    "terminal.ansiBrightWhite": "#DFE3E6",
    "gitDecoration.addedResourceForeground": "#8BD0B5",
    "gitDecoration.modifiedResourceForeground": "#E6C79C",
    "gitDecoration.deletedResourceForeground": "#8DB0D8",
    "gitDecoration.untrackedResourceForeground": "#8BD0B5",
    "gitDecoration.ignoredResourceForeground": "#6C7680",
    "gitDecoration.conflictingResourceForeground": "#8DB0D8",
    "gitDecoration.submoduleResourceForeground": "#8BD0B5"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6C7680",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#9BB8FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#E6C79C"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#D9DEE2"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#8BD0B5"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#B6C4D3"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#8DB0D8"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#94A1AE"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C85E5E"
      }
    }
  ]
}