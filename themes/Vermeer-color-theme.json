{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vermeer",
  "type": "light",
  "colors": {
    "editor.background": "#F7F4EB",
    "editor.foreground": "#283043",
    "editorCursor.foreground": "#9B7B2F",
    "editor.selectionBackground": "#E3D7BD",
    "editor.inactiveSelectionBackground": "#EAE0CB",
    "editor.lineHighlightBackground": "#EFE9D8",
    "editorLineNumber.foreground": "#9C9AA2",
    "editorLineNumber.activeForeground": "#9B7B2F",
    "activityBar.background": "#EFE7D7",
    "activityBar.foreground": "#283043",
    "sideBar.background": "#F3EDE0",
    "sideBar.foreground": "#283043",
    "statusBar.background": "#5773A6",
    "statusBar.foreground": "#0F1726",
    "titleBar.activeBackground": "#E5DAC7",
    "titleBar.activeForeground": "#283043",
    "tab.activeBackground": "#F7F4EB",
    "tab.activeForeground": "#283043",
    "tab.inactiveBackground": "#EAE2D3",
    "tab.inactiveForeground": "#6A6271",
    "panel.background": "#F1E9DB",
    "panel.border": "#D8CFBD",
    "button.background": "#9B7B2F",
    "button.foreground": "#1B150A",
    "badge.background": "#5773A6",
    "badge.foreground": "#0F1726",
    "input.background": "#FCF9F2",
    "input.border": "#DDD3C2",
    "focusBorder": "#5773A6",
    "selection.background": "#E3D7BD",
    "list.activeSelectionBackground": "#E3D7BD",
    "list.activeSelectionForeground": "#283043",
    "list.inactiveSelectionBackground": "#EAE0CB",
    "list.hoverBackground": "#EDE6D4",
    "list.focusBackground": "#E3D7BD",
    "list.dropBackground": "#5773A633",
    "list.highlightForeground": "#5773A6",
    "editor.findMatchBackground": "#5773A640",
    "editor.findMatchBorder": "#5773A6",
    "editor.findMatchHighlightBackground": "#5773A625",
    "editor.findMatchHighlightBorder": "#5773A666",
    "editor.findRangeHighlightBackground": "#5773A615",
    "editor.findRangeHighlightBorder": "#5773A640",
    "diffEditor.insertedTextBackground": "#3E6B7333",
    "diffEditor.removedTextBackground": "#9B7B2F33",
    "diffEditor.insertedLineBackground": "#3E6B7326",
    "diffEditor.removedLineBackground": "#9B7B2F26",
    "diffEditorGutter.insertedLineBackground": "#3E6B73",
    "diffEditorGutter.removedLineBackground": "#9B7B2F",
    "diffEditorGutter.modifiedLineBackground": "#B1872C",
    "diffEditor.diagonalFill": "#5C566340",
    "terminal.background": "#F1E9DB",
    "terminal.foreground": "#283043",
    "terminalCursor.foreground": "#5773A6",
    "terminal.ansiBlack": "#C3C3C1",
    "terminal.ansiRed": "#9B7B2F",
    "terminal.ansiGreen": "#3E6B73",
    "terminal.ansiYellow": "#B1872C",
    "terminal.ansiBlue": "#5773A6",
    "terminal.ansiMagenta": "#4C6FAE",
    "terminal.ansiCyan": "#3E6B73",
    "terminal.ansiWhite": "#283043",
    "terminal.ansiBrightBlack": "#D8D8D7",
    "terminal.ansiBrightRed": "#B49C63",
    "terminal.ansiBrightGreen": "#6E9096",
    "terminal.ansiBrightYellow": "#C19F56",
    "terminal.ansiBrightBlue": "#798FB8",
    "terminal.ansiBrightMagenta": "#7993C2",
    "terminal.ansiBrightCyan": "#6E9096",
    "terminal.ansiBrightWhite": "#484F5F",
    "gitDecoration.addedResourceForeground": "#3E6B73",
    "gitDecoration.modifiedResourceForeground": "#B1872C",
    "gitDecoration.deletedResourceForeground": "#9B7B2F",
    "gitDecoration.untrackedResourceForeground": "#3E6B73",
    "gitDecoration.ignoredResourceForeground": "#5C5663",
    "gitDecoration.conflictingResourceForeground": "#9B7B2F",
    "gitDecoration.submoduleResourceForeground": "#5773A6"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7D7A82",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#5773A6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#B1872C"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#283043"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#4C6FAE"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#3E6B73"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#9B7B2F"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5C5663"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C85A3C"
      }
    }
  ]
}