{
  "$schema": "vscode://schemas/color-theme",
  "name": "Satie",
  "type": "light",
  "colors": {
    "editor.background": "#F8FAFB",
    "editor.foreground": "#1F262B",
    "editorCursor.foreground": "#6FB7A6",
    "editor.selectionBackground": "#D8E9E2",
    "editor.inactiveSelectionBackground": "#E5F0EC",
    "editor.lineHighlightBackground": "#EEF3F6",
    "editorLineNumber.foreground": "#9AA6AF",
    "editorLineNumber.activeForeground": "#6FB7A6",
    "activityBar.background": "#EEF2F5",
    "activityBar.foreground": "#1F262B",
    "sideBar.background": "#F3F6F8",
    "sideBar.foreground": "#1F262B",
    "statusBar.background": "#C3D7D0",
    "statusBar.foreground": "#1A2125",
    "titleBar.activeBackground": "#E3EAED",
    "titleBar.activeForeground": "#1F262B",
    "tab.activeBackground": "#F8FAFB",
    "tab.activeForeground": "#1F262B",
    "tab.inactiveBackground": "#E7EDF0",
    "tab.inactiveForeground": "#6A747C",
    "panel.background": "#F5F8FA",
    "panel.border": "#D5E1E3",
    "button.background": "#6FB7A6",
    "button.foreground": "#0F1619",
    "badge.background": "#9DC7BC",
    "badge.foreground": "#102018",
    "input.background": "#FBFDFD",
    "input.border": "#D5E2E0",
    "focusBorder": "#6FB7A6",
    "selection.background": "#D8E9E2",
    "list.activeSelectionBackground": "#D8E9E2",
    "list.activeSelectionForeground": "#1F262B",
    "list.inactiveSelectionBackground": "#E5F0EC",
    "list.hoverBackground": "#E8F2EE",
    "list.focusBackground": "#D8E9E2",
    "list.dropBackground": "#5E7FA633",
    "list.highlightForeground": "#5E7FA6",
    "editor.findMatchBackground": "#5E7FA640",
    "editor.findMatchBorder": "#5E7FA6",
    "editor.findMatchHighlightBackground": "#5E7FA625",
    "editor.findMatchHighlightBorder": "#5E7FA666",
    "editor.findRangeHighlightBackground": "#5E7FA615",
    "editor.findRangeHighlightBorder": "#5E7FA640",
    "diffEditor.insertedTextBackground": "#5B9C8D33",
    "diffEditor.removedTextBackground": "#5B8EA333",
    "diffEditor.insertedLineBackground": "#5B9C8D26",
    "diffEditor.removedLineBackground": "#5B8EA326",
    "diffEditorGutter.insertedLineBackground": "#5B9C8D",
    "diffEditorGutter.removedLineBackground": "#5B8EA3",
    "diffEditorGutter.modifiedLineBackground": "#B38C7A",
    "diffEditor.diagonalFill": "#7C8A9440",
    "terminal.background": "#F5F8FA",
    "terminal.foreground": "#1F262B",
    "terminalCursor.foreground": "#5E7FA6",
    "terminal.ansiBlack": "#C2C5C7",
    "terminal.ansiRed": "#5B8EA3",
    "terminal.ansiGreen": "#5B9C8D",
    "terminal.ansiYellow": "#B38C7A",
    "terminal.ansiBlue": "#5E7FA6",
    "terminal.ansiMagenta": "#5E686F",
    "terminal.ansiCyan": "#5B9C8D",
    "terminal.ansiWhite": "#1F262B",
    "terminal.ansiBrightBlack": "#D7D9DB",
    "terminal.ansiBrightRed": "#84AABA",
    "terminal.ansiBrightGreen": "#84B5AA",
    "terminal.ansiBrightYellow": "#C2A395",
    "terminal.ansiBrightBlue": "#7E99B8",
    "terminal.ansiBrightMagenta": "#868E93",
    "terminal.ansiBrightCyan": "#84B5AA",
    "terminal.ansiBrightWhite": "#41474B",
    "gitDecoration.addedResourceForeground": "#5B9C8D",
    "gitDecoration.modifiedResourceForeground": "#B38C7A",
    "gitDecoration.deletedResourceForeground": "#5B8EA3",
    "gitDecoration.untrackedResourceForeground": "#5B9C8D",
    "gitDecoration.ignoredResourceForeground": "#7C8A94",
    "gitDecoration.conflictingResourceForeground": "#5B8EA3",
    "gitDecoration.submoduleResourceForeground": "#5E7FA6"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7C8A94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#5E7FA6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#B38C7A"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#1F262B"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#5B9C8D"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#627A99"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#5B8EA3"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5E686F"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C85E5E"
      }
    }
  ]
}