{
  "$schema": "vscode://schemas/color-theme",
  "name": "Monet",
  "type": "light",
  "colors": {
    "editor.background": "#F6F8F4",
    "editor.foreground": "#2F3431",
    "editorCursor.foreground": "#76AB9C",
    "editor.selectionBackground": "#D4E4EB",
    "editor.inactiveSelectionBackground": "#E3EDF2",
    "editor.lineHighlightBackground": "#ECF2EC",
    "editorLineNumber.foreground": "#9CA8A1",
    "editorLineNumber.activeForeground": "#76AB9C",
    "activityBar.background": "#E7EDE7",
    "activityBar.foreground": "#2F3431",
    "sideBar.background": "#F0F4EE",
    "sideBar.foreground": "#2F3431",
    "statusBar.background": "#B8CCBF",
    "statusBar.foreground": "#1E2521",
    "titleBar.activeBackground": "#E1E9E0",
    "titleBar.activeForeground": "#2F3431",
    "tab.activeBackground": "#F6F8F4",
    "tab.activeForeground": "#2F3431",
    "tab.inactiveBackground": "#E9EFEA",
    "tab.inactiveForeground": "#6D7570",
    "panel.background": "#F3F6F0",
    "panel.border": "#D7E1DA",
    "button.background": "#76AB9C",
    "button.foreground": "#0F1813",
    "badge.background": "#A3C7D7",
    "badge.foreground": "#1F2B32",
    "input.background": "#FAFCFA",
    "input.border": "#D8E3DB",
    "focusBorder": "#76AB9C",
    "selection.background": "#D4E4EB",
    "list.activeSelectionBackground": "#D4E4EB",
    "list.activeSelectionForeground": "#2F3431",
    "list.inactiveSelectionBackground": "#E3EDF2",
    "list.hoverBackground": "#E4EEF0",
    "list.focusBackground": "#D4E4EB",
    "list.dropBackground": "#4E81A133",
    "list.highlightForeground": "#4E81A3",
    "editor.findMatchBackground": "#5B8CB040",
    "editor.findMatchBorder": "#5B8CB0",
    "editor.findMatchHighlightBackground": "#5B8CB025",
    "editor.findMatchHighlightBorder": "#5B8CB066",
    "editor.findRangeHighlightBackground": "#5B8CB015",
    "editor.findRangeHighlightBorder": "#5B8CB040",
    "diffEditor.insertedTextBackground": "#72AA9340",
    "diffEditor.removedTextBackground": "#D7A7B640",
    "diffEditor.insertedLineBackground": "#72AA932B",
    "diffEditor.removedLineBackground": "#D7A7B62B",
    "diffEditorGutter.insertedLineBackground": "#72AA93",
    "diffEditorGutter.removedLineBackground": "#D7A7B6",
    "diffEditorGutter.modifiedLineBackground": "#6E90B6",
    "diffEditor.diagonalFill": "#616A6640",
    "terminal.background": "#F3F6F0",
    "terminal.foreground": "#2F3431",
    "terminalCursor.foreground": "#4E81A3",
    "terminal.ansiBlack": "#C6CAC4",
    "terminal.ansiRed": "#D7A7B6",
    "terminal.ansiGreen": "#72AA93",
    "terminal.ansiYellow": "#CCB27A",
    "terminal.ansiBlue": "#5B8CB0",
    "terminal.ansiMagenta": "#4F7F8A",
    "terminal.ansiCyan": "#72AA93",
    "terminal.ansiWhite": "#2F3431",
    "terminal.ansiBrightBlack": "#DBDDDA",
    "terminal.ansiBrightRed": "#E2BDCA",
    "terminal.ansiBrightGreen": "#93BEAC",
    "terminal.ansiBrightYellow": "#D7C28C",
    "terminal.ansiBrightBlue": "#7A9FC0",
    "terminal.ansiBrightMagenta": "#7DA7AE",
    "terminal.ansiBrightCyan": "#93BEAC",
    "terminal.ansiBrightWhite": "#4F5350",
    "gitDecoration.addedResourceForeground": "#72AA93",
    "gitDecoration.modifiedResourceForeground": "#CCB27A",
    "gitDecoration.deletedResourceForeground": "#D7A7B6",
    "gitDecoration.untrackedResourceForeground": "#72AA93",
    "gitDecoration.ignoredResourceForeground": "#616A66",
    "gitDecoration.conflictingResourceForeground": "#CCB27A",
    "gitDecoration.submoduleResourceForeground": "#5B8CB0"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7B9480",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#5B8CB0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#D7A7B6"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#2F3431"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#4E81A3"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#4F888F"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#72AA93"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#616A66"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C25E5A"
      }
    }
  ]
}