{
  "$schema": "vscode://schemas/color-theme",
  "name": "Monet",
  "type": "light",
  "colors": {
    "editor.background": "#F5F7F2",
    "editor.foreground": "#2E312F",
    "editorCursor.foreground": "#7CB3A3",
    "editor.selectionBackground": "#C9DCE4",
    "editor.inactiveSelectionBackground": "#D9E6EC",
    "editor.lineHighlightBackground": "#EAEFE8",
    "editorLineNumber.foreground": "#9BA6A0",
    "editorLineNumber.activeForeground": "#7CB3A3",
    "activityBar.background": "#E5ECE5",
    "activityBar.foreground": "#2E312F",
    "sideBar.background": "#EEF2EC",
    "sideBar.foreground": "#2E312F",
    "statusBar.background": "#B4C9BE",
    "statusBar.foreground": "#1F2522",
    "titleBar.activeBackground": "#DDE7DD",
    "titleBar.activeForeground": "#2E312F",
    "tab.activeBackground": "#F5F7F2",
    "tab.activeForeground": "#2E312F",
    "tab.inactiveBackground": "#E6ECE7",
    "tab.inactiveForeground": "#6C7470",
    "panel.background": "#F1F4EE",
    "panel.border": "#D5DFD8",
    "button.background": "#7CB3A3",
    "button.foreground": "#0F1813",
    "badge.background": "#A5C4D4",
    "badge.foreground": "#1E2A33",
    "input.background": "#F8FBF8",
    "input.border": "#D6E2DA",
    "focusBorder": "#7CB3A3",
    "selection.background": "#C9DCE4",
    "list.activeSelectionBackground": "#C9DCE4",
    "list.activeSelectionForeground": "#2E312F",
    "list.inactiveSelectionBackground": "#D9E6EC",
    "list.hoverBackground": "#DFEAEB",
    "list.focusBackground": "#C9DCE4",
    "list.dropBackground": "#4C7FA133",
    "list.highlightForeground": "#4C7FA1",
    "editor.findMatchBackground": "#4C7FA140",
    "editor.findMatchBorder": "#4C7FA1",
    "editor.findMatchHighlightBackground": "#4C7FA125",
    "editor.findMatchHighlightBorder": "#4C7FA166",
    "editor.findRangeHighlightBackground": "#4C7FA115",
    "editor.findRangeHighlightBorder": "#4C7FA140",
    "diffEditor.insertedTextBackground": "#6BA58F33",
    "diffEditor.removedTextBackground": "#D5A3B533",
    "diffEditor.insertedLineBackground": "#6BA58F26",
    "diffEditor.removedLineBackground": "#D5A3B526",
    "diffEditorGutter.insertedLineBackground": "#6BA58F",
    "diffEditorGutter.removedLineBackground": "#D5A3B5",
    "diffEditorGutter.modifiedLineBackground": "#D5A3B5",
    "diffEditor.diagonalFill": "#5C666240",
    "terminal.background": "#F1F4EE",
    "terminal.foreground": "#2E312F",
    "terminalCursor.foreground": "#4C7FA1",
    "terminal.ansiBlack": "#C3C6C1",
    "terminal.ansiRed": "#D5A3B5",
    "terminal.ansiGreen": "#6BA58F",
    "terminal.ansiYellow": "#D5A3B5",
    "terminal.ansiBlue": "#4C7FA1",
    "terminal.ansiMagenta": "#487B84",
    "terminal.ansiCyan": "#6BA58F",
    "terminal.ansiWhite": "#2E312F",
    "terminal.ansiBrightBlack": "#D8DAD7",
    "terminal.ansiBrightRed": "#E0BAC8",
    "terminal.ansiBrightGreen": "#90BCAB",
    "terminal.ansiBrightYellow": "#DDB5C4",
    "terminal.ansiBrightBlue": "#7099B4",
    "terminal.ansiBrightMagenta": "#769CA3",
    "terminal.ansiBrightCyan": "#90BCAB",
    "terminal.ansiBrightWhite": "#4D504E",
    "gitDecoration.addedResourceForeground": "#6BA58F",
    "gitDecoration.modifiedResourceForeground": "#D5A3B5",
    "gitDecoration.deletedResourceForeground": "#D5A3B5",
    "gitDecoration.untrackedResourceForeground": "#6BA58F",
    "gitDecoration.ignoredResourceForeground": "#5C6662",
    "gitDecoration.conflictingResourceForeground": "#D5A3B5",
    "gitDecoration.submoduleResourceForeground": "#4C7FA1"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#76927B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#4F7AA1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#D5A3B5"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#2E312F"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#4C7FA1"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#487B84"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#6BA58F"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5C6662"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#C7514C"
      }
    }
  ]
}