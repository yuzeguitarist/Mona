{
  "$schema": "vscode://schemas/color-theme",
  "name": "Da Vinci",
  "type": "dark",
  "colors": {
    "editor.background": "#1F1A13",
    "editor.foreground": "#E8D8B6",
    "editorCursor.foreground": "#B68452",
    "editor.selectionBackground": "#3A2F22",
    "editor.inactiveSelectionBackground": "#2F271D",
    "editor.lineHighlightBackground": "#2C241A",
    "editorLineNumber.foreground": "#A18C6D",
    "editorLineNumber.activeForeground": "#C9A66B",
    "activityBar.background": "#251D16",
    "activityBar.foreground": "#E8D8B6",
    "sideBar.background": "#201912",
    "sideBar.foreground": "#E8D8B6",
    "statusBar.background": "#7E705C",
    "statusBar.foreground": "#0F0B07",
    "titleBar.activeBackground": "#2D241A",
    "titleBar.activeForeground": "#E8D8B6",
    "tab.activeBackground": "#1F1A13",
    "tab.activeForeground": "#E8D8B6",
    "tab.inactiveBackground": "#261F16",
    "tab.inactiveForeground": "#B8A07D",
    "panel.background": "#221C14",
    "panel.border": "#352A1E",
    "button.background": "#B68452",
    "button.foreground": "#130E08",
    "badge.background": "#C9A66B",
    "badge.foreground": "#1C150E",
    "input.background": "#241D14",
    "input.border": "#3A2E21",
    "focusBorder": "#B68452",
    "selection.background": "#3A2F22",
    "list.activeSelectionBackground": "#3A2F22",
    "list.activeSelectionForeground": "#E8D8B6",
    "list.inactiveSelectionBackground": "#2F271D",
    "list.hoverBackground": "#2C241A",
    "list.focusBackground": "#3A2F22",
    "list.dropBackground": "#C9A66B33",
    "list.highlightForeground": "#C9A66B",
    "editor.findMatchBackground": "#C9A66B40",
    "editor.findMatchBorder": "#C9A66B",
    "editor.findMatchHighlightBackground": "#C9A66B25",
    "editor.findMatchHighlightBorder": "#C9A66B66",
    "editor.findRangeHighlightBackground": "#C9A66B15",
    "editor.findRangeHighlightBorder": "#C9A66B40",
    "diffEditor.insertedTextBackground": "#9BB08033",
    "diffEditor.removedTextBackground": "#C28E5E33",
    "diffEditor.insertedLineBackground": "#9BB08026",
    "diffEditor.removedLineBackground": "#C28E5E26",
    "diffEditorGutter.insertedLineBackground": "#9BB080",
    "diffEditorGutter.removedLineBackground": "#C28E5E",
    "diffEditorGutter.modifiedLineBackground": "#D1B27A",
    "diffEditor.diagonalFill": "#B7A17D40",
    "terminal.background": "#221C14",
    "terminal.foreground": "#E8D8B6",
    "terminalCursor.foreground": "#C9A66B",
    "terminal.ansiBlack": "#514A3C",
    "terminal.ansiRed": "#C28E5E",
    "terminal.ansiGreen": "#9BB080",
    "terminal.ansiYellow": "#D1B27A",
    "terminal.ansiBlue": "#C9A66B",
    "terminal.ansiMagenta": "#8FA7A0",
    "terminal.ansiCyan": "#9BB080",
    "terminal.ansiWhite": "#E8D8B6",
    "terminal.ansiBrightBlack": "#8E8980",
    "terminal.ansiBrightRed": "#D1AA86",
    "terminal.ansiBrightGreen": "#B4C4A0",
    "terminal.ansiBrightYellow": "#DAC195",
    "terminal.ansiBrightBlue": "#D4B889",
    "terminal.ansiBrightMagenta": "#ABBDB8",
    "terminal.ansiBrightCyan": "#B4C4A0",
    "terminal.ansiBrightWhite": "#EBDEC1",
    "gitDecoration.addedResourceForeground": "#9BB080",
    "gitDecoration.modifiedResourceForeground": "#D1B27A",
    "gitDecoration.deletedResourceForeground": "#C28E5E",
    "gitDecoration.untrackedResourceForeground": "#9BB080",
    "gitDecoration.ignoredResourceForeground": "#B7A17D",
    "gitDecoration.conflictingResourceForeground": "#C28E5E",
    "gitDecoration.submoduleResourceForeground": "#C9A66B"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9B8A70",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#C9A66B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#9BB080"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#E8D8B6"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#C28E5E"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#8FA7A0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#D1B27A"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#B7A17D"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#A4493E"
      }
    }
  ]
}