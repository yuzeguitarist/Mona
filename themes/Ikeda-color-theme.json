{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ikeda",
  "type": "light",
  "colors": {
    "editor.background": "#F7F9FA",
    "editor.foreground": "#1F262C",
    "editorCursor.foreground": "#78BCCD",
    "editor.selectionBackground": "#E4F0F4",
    "editor.inactiveSelectionBackground": "#EEF6F8",
    "editor.lineHighlightBackground": "#EFF3F5",
    "editorLineNumber.foreground": "#A3ADB5",
    "editorLineNumber.activeForeground": "#78BCCD",
    "activityBar.background": "#F1F5F7",
    "activityBar.foreground": "#1F262C",
    "sideBar.background": "#F6F8F9",
    "sideBar.foreground": "#1F262C",
    "statusBar.background": "#D5E5EB",
    "statusBar.foreground": "#151E24",
    "titleBar.activeBackground": "#E8F0F3",
    "titleBar.activeForeground": "#1F262C",
    "tab.activeBackground": "#F7F9FA",
    "tab.activeForeground": "#1F262C",
    "tab.inactiveBackground": "#EDEFF2",
    "tab.inactiveForeground": "#70797F",
    "panel.background": "#F7F9FA",
    "panel.border": "#D9E4EA",
    "button.background": "#8CCBD8",
    "button.foreground": "#142029",
    "badge.background": "#A3D6E1",
    "badge.foreground": "#0F191F",
    "input.background": "#FFFFFF",
    "input.border": "#DCE7EC",
    "focusBorder": "#8CCBD8",
    "selection.background": "#E4F0F4",
    "list.activeSelectionBackground": "#E4F0F4",
    "list.activeSelectionForeground": "#1F262C",
    "list.inactiveSelectionBackground": "#EEF6F8",
    "list.hoverBackground": "#EEF4F7",
    "list.focusBackground": "#E4F0F4",
    "list.dropBackground": "#5A819333",
    "list.highlightForeground": "#5E89A8",
    "editor.findMatchBackground": "#5E89A830",
    "editor.findMatchBorder": "#5E89A8",
    "editor.findMatchHighlightBackground": "#5E89A820",
    "editor.findMatchHighlightBorder": "#5E89A855",
    "editor.findRangeHighlightBackground": "#5E89A814",
    "editor.findRangeHighlightBorder": "#5E89A830",
    "diffEditor.insertedTextBackground": "#78BCCD30",
    "diffEditor.removedTextBackground": "#6A7C8D30",
    "diffEditor.insertedLineBackground": "#78BCCD22",
    "diffEditor.removedLineBackground": "#6A7C8D22",
    "diffEditorGutter.insertedLineBackground": "#78BCCD",
    "diffEditorGutter.removedLineBackground": "#6A7C8D",
    "diffEditorGutter.modifiedLineBackground": "#9A8676",
    "diffEditor.diagonalFill": "#5C666D33",
    "terminal.background": "#F7F9FA",
    "terminal.foreground": "#1F262C",
    "terminalCursor.foreground": "#5E89A8",
    "terminal.ansiBlack": "#C6C9CB",
    "terminal.ansiRed": "#8090A0",
    "terminal.ansiGreen": "#78BCCD",
    "terminal.ansiYellow": "#9A8676",
    "terminal.ansiBlue": "#5E89A8",
    "terminal.ansiMagenta": "#6B94B5",
    "terminal.ansiCyan": "#78BCCD",
    "terminal.ansiWhite": "#1F262C",
    "terminal.ansiBrightBlack": "#DBDCDD",
    "terminal.ansiBrightRed": "#9BAABA",
    "terminal.ansiBrightGreen": "#8CCBD8",
    "terminal.ansiBrightYellow": "#AFA195",
    "terminal.ansiBrightBlue": "#7BA1BB",
    "terminal.ansiBrightMagenta": "#88AFC8",
    "terminal.ansiBrightCyan": "#8CCBD8",
    "terminal.ansiBrightWhite": "#3B4146",
    "gitDecoration.addedResourceForeground": "#78BCCD",
    "gitDecoration.modifiedResourceForeground": "#9A8676",
    "gitDecoration.deletedResourceForeground": "#6A7C8D",
    "gitDecoration.untrackedResourceForeground": "#78BCCD",
    "gitDecoration.ignoredResourceForeground": "#5C666D",
    "gitDecoration.conflictingResourceForeground": "#6A7C8D",
    "gitDecoration.submoduleResourceForeground": "#5E89A8"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7F8A92",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#5E89A8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.character"
      ],
      "settings": {
        "foreground": "#9A8676"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#1F262C"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#78BCCD"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#6A7C8D"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#6B94B5"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5C666D"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#D77B7B"
      }
    }
  ]
}